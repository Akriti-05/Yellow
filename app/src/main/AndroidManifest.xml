<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.techsters.aasthaapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.AasthaApp">
        <activity android:name="com.techsters.aasthaapp.TipsActivity"></activity>
        <activity android:name="com.techsters.aasthaapp.MusicActivity" />
        <activity android:name="com.techsters.aasthaapp.IntroActivity" />
        <activity android:name="com.techsters.aasthaapp.PostActivity" >

        </activity>



        <activity android:name="com.techsters.aasthaapp.SettingsActivity" />
        <activity android:name="com.techsters.aasthaapp.splashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name="com.techsters.aasthaapp.chatDetailActivity" />
        <activity android:name="com.techsters.aasthaapp.aboutActivity" />
        <activity android:name="com.techsters.aasthaapp.SignInActivity" />
        <activity android:name="com.techsters.aasthaapp.signUPActivity" />
        <activity android:name="com.techsters.aasthaapp.MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <service
            android:name="com.techsters.aasthaapp.Models.MyFirebaseMessagingService"
            android:directBootAware="true"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>